{
  "version": 2,
  "builds": [
    {
      "src": "backend/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "buildCommand": "cd backend && npm install && npm run build",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/dist/main.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
